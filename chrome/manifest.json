{
  "manifest_version": 3,
  "name": "KILL Bilibili search placeholder",
  "description": "Stop showing me your DAMN default search recommendations!",
  "version": "1.0.0",
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "declarative_net_request" : {
    "rule_resources" : [{
      "id": "bilibili_search_default_ruleset",
      "enabled": true,
      "path": "rules/rule.json"
    }]
  },

  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestFeedback"
  ]
}
